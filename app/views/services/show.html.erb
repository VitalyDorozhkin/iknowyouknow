



<%= render "layouts/service", service: @service %>
<%if @service.teacher_id == current_user %>
  <%= link_to "EDIT", edit_service_path(@service.id) %>
  <%= link_to "DELETE", service_path(@service.id), method: "DELETE" %>
<%elsif !@self_user.student_service_ids.include?(@service.id)%>
  <%= link_to "JOIN", new_order_path(service_id: @service.id) %>
<%end %>









